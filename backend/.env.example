# RUHANI Backend Environment Variables
# Copy this file to .env and fill in your actual API keys
# DO NOT commit your .env file to git!

# =============================================================================
# GROQ API (LLM & STT)
# =============================================================================
# Get your API key from: https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here
GROQ_API_BASE_URL=https://api.groq.com/v1

# =============================================================================
# ELEVENLABS API (Text-to-Speech)
# =============================================================================
# Get your API key from: https://elevenlabs.io/
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_API_BASE_URL=https://api.elevenlabs.io/v1

# =============================================================================
# FETCH.AI API (Public Info Gathering)
# =============================================================================
# Get your API key from: https://console.fetch.ai/
FETCHAI_API_KEY=your_fetchai_api_key_here
FETCHAI_API_BASE_URL=https://api.fetch.ai/v1

# =============================================================================
# SNOWFLAKE DATABASE
# =============================================================================
# Get these from your Snowflake account: https://app.snowflake.com/
SNOWFLAKE_ACCOUNT=your_account_identifier_here
SNOWFLAKE_USER=your_username_here
SNOWFLAKE_PASSWORD=your_password_here
SNOWFLAKE_WAREHOUSE=your_warehouse_name_here
SNOWFLAKE_DATABASE=your_database_name_here
SNOWFLAKE_SCHEMA=your_schema_name_here
SNOWFLAKE_ROLE=your_role_name_here

# =============================================================================
# CORAL PROTOCOL (Activity Logging)
# =============================================================================
# Get your API key from: https://coralprotocol.com/
CORAL_API_KEY=your_coral_api_key_here
CORAL_API_BASE_URL=https://api.coralprotocol.com/v1

# =============================================================================
# JWT SECRET (Authentication)
# =============================================================================
# Generate a strong random string for JWT signing
JWT_SECRET=your_jwt_secret_here

# =============================================================================
# FRONTEND CONFIGURATION
# =============================================================================
# URL where your backend is running
VITE_BACKEND_URL=http://localhost:8000 